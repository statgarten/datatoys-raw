						<연계항목 type="이미지" 연계시작="ITKC_MO_1237A_B137_350C" 연계종료="ITKC_MO_1237A_B137_351A"/>
					</연계정보>
				</레벨4>
				<레벨4 type="최종정보" id="ITKC_MO_1237A_0050_010_0170" 자료생성일="2020-10-15" DCI="ITKC_MO_1237A_0050_010_0170_2020_B137_XML">
